let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/Labs/ewestudios/ewe_platform
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +76 ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js
badd +1 backends/foundation_core/src/megatron/jsdom/packages/packer.js
badd +40 ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/mod.rs
argglobal
%argdel
edit ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/mod.rs
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
wincmd =
argglobal
balt ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js
setlocal fdm=expr
setlocal fde=nvim_treesitter#foldexpr()
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=99
setlocal fml=1
setlocal fdn=20
setlocal fen
let s:l = 40 - ((39 * winheight(0) + 36) / 72)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 40
normal! 05|
wincmd w
argglobal
if bufexists(fnamemodify("~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js", ":p")) | buffer ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js | else | edit ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js | endif
if &buftype ==# 'terminal'
  silent file ~/Labs/ewestudios/ewe_platform/backends/foundation_core/src/megatron/jsdom/packages/arena.js
endif
setlocal fdm=expr
setlocal fde=nvim_treesitter#foldexpr()
